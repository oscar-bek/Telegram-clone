+			</Button>
+			<Button variant={'secondary'} onClick={() => onSignIn('github')} disabled={isLoading}>
+				<span>Sign up with github</span>
+				<FaGithub />
+			</Button>
+		</div>
+	)
+}
+
+export default Social
